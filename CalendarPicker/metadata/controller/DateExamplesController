                    "        xtype: 'button',\r",
                    "        text: 'RETURN',\r",
                    "        iconCls: '',\r",
                    "        cls: 'pickerfooterbtns',\r",
                    "        listeners : {\r",
                    "            tap: function (button, e, options) {\r",
