                    "gridlist.setItemHeight(pArr[newIdx].itemHeight);",
                    "gridlistX.setItemHeight(pArr[newIdx].itemHeight);",
                    "",
                    "gridcont.setWidth(pArr[newIdx].width);",
                    "gridlist.setWidth(pArr[newIdx].width);",
                    "",
