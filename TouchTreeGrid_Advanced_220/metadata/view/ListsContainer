                    "\r",
                    "// get screen width in pixels\r",
                    "var scrnwid = (window.innerWidth > 0) ? window.innerWidth : screen.width;  \r",
                    "// get maximum of screen width or dow2012horiz grid width converted to pixels\r",
                    "var prewid = 36 * 16;  // 36em total width * 16 pixels per em\r",
                    "var minScrnWid = Math.max(prewid, scrnwid);  \r",
                    "\r",
                    "horizgrid.setWidth(minScrnWid); \r",
                    "horizgrid.setMinWidth(minScrnWid); \r",
                    "horizexample.setWidth(minScrnWid); \r",
                    "horizexample.setMinWidth(minScrnWid); \r",
                    "\r",
                    "if (scrnwid > prewid) {\r",
